export fn main() {
    let len: i32 = 5;
    let arr: [i32; 5] = [3, 1, 2, 4, 5];
    let mut i: u32 = 0;
    let mut sum: u32 = 0;

    while i < len {
        sum += arr[i] as u32;
        i += 1;
    }

    let a: f32 = sum as f32;
    let b: f32 = 41.0;
    let d: i32 = 0x1E;

    __builtin_print_f32((a + b) / ((d * 2) as f32));
}
