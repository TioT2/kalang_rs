declare mul_f32 = |first: f32, second: f32| -> f32 {
    first * second
};

declare lhs: f32 = 7.0;
declare rhs: f32 = 9.0;

declare __builtin = space {
    declare print_f32 = extern;
    declare print_i32 = extern;
};

declare main = || -> i32 {
    const m: f32 = mul_f32(lhs, rhs);

    __builtin::print_f32(m);
    __builtin::print_i32(m as i32);
    0
};
